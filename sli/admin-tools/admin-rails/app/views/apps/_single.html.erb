<dl class="dl-horizontal">
  <dt>Client Id</dt>
  <dd><%= client_field_value(app, :client_id) %></dd>
  <dt>Shared Secret</dt>
  <dd><%= client_field_value(app, :client_secret) %></dd>
  <dt>Description</dt>
  <dd><%= app.description %></dd>
  <dt>Version</dt>
  <dd><%= app.version %></dd>
  <dt>Vendor</dt>
  <dd><%= app.vendor %></dd>
  <dt>Url</dt>
  <dd><%= app.application_url %></dd>
  <dt>Admin Url</dt>
  <dd><%= app.administration_url %></dd>
  <dt>Redirect Url</dt>
  <dd><%= app.redirect_uri %></dd>
  <dt>Application Icon Url</dt>
  <dd><%= app.image_url %></dd>
  <dt>Admin application?</dt>
  <dd><%= app.is_admin %></dd>
  <dt>Installed application?</dt>
  <dd><%= app.installed %></dd>
  <dt>Display method</dt>
  <dd><%= app.behavior %></dd>
</dl>