<span class="district-text">Districts in <%= sea %></span>
<div class="enable-disable"><a id="enable-all" href="#">Enable All</a> | <a id="disable-all" href="#">Disable All</a></div>


<table class="table-striped table">
  <thead>
  </thead>
  <tbody>
    <% results.each do |lea| %>
    <tr id="<%= lea["id"]%>">
      <td>
      <label class="checkbox inline">
      <input type="checkbox"><%=lea["name"]%></input>
      </label>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
