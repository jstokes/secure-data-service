<table>
	<tr>
	<th>Name</th>
	<th>Version</th>
	<th>Vendor</th>
	<th>Creation Date</th>
	<th>Last Update</th>
	<th>Actions</th>
	</tr>
	<% @apps.each do |app| %>
		<tr>
	    <td><%= app.name %></td>
	    <td><%= app.version %></td>
	    <td><%= app.created_on %></td>
		<td><%= app.updated_on %></td>
		<td><%= link_to 'Edit', edit_app_path(app) %><% link_to 'Delete', app, :remote => true,  :confirm => "Are you sure?", :method => :destroy %></td>
		</tr>
	<% end %>
</table>