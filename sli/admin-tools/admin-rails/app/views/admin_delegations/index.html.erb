  <h1>District <%= @admin_delegation.localEdOrgId  %> - Delegate District Privileges to a State Super Administrator</h1>

  <%= form_for(@admin_delegation) do |f| %>
    <%= f.hidden_field :localEdOrgId %>
      <table>

      <tr>
          <td><%= f.check_box :appApprovalEnabled %>    </td>
          <td>Approve Applications</td>
      </tr>

      <tr>
          <td><%= f.check_box :viewSecurityEventsEnabled %></td>
          <td>View Security Events</td>
      </tr>
      </table>

      <div class="field">
        <%= f.submit "Save" %>
      </div>

  <% end %>

