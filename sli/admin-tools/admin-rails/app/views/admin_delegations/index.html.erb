
  <h1>District <%= @admin_delegation.localEdOrgId  %> - Delegate District Privileges</h1>



  <%= form_for(@admin_delegation) do |f| %>
    <%= f.hidden_field :localEdOrgId %>
      <table>
      <tr>
        <td><%= f.check_box :ingestDataEnabled %>  </td>
        <td><%= f.label :ingestDataEnabled, "Ingest District's Data" %> </td>
      </tr>

      <tr>
          <td><%= f.check_box :appApprovalEnabled %>    </td>
          <td><%= f.label :appApprovalEnabled, "Approve Applications" %> </td>
      </tr>

      <tr>
          <td><%= f.check_box :viewSecurityEventsEnabled %></td>
          <td><%= f.label :viewSecurityEventsEnabled, "View Security Events" %></td>
      </tr>
      </table>

      <div class="field">
        <%= f.submit "Save" %>
      </div>

  <% end %>

