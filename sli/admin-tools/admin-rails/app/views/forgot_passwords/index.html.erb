<div class="page-header">
<h1>Reset Password</h1>
</div>
<p></p>
<%= form_tag(:action => "reset") do %>
  <%= label_tag(:user_id, "Please enter your user id (this is probably your email address).") %>
  <%= text_field_tag(:user_id) %><br><br>
  <%= submit_tag("Submit", :id => "submit") %>
<% end %>
