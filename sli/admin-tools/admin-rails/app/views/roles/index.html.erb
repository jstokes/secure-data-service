<h1>Default SLC Roles</h1>

<table class="table table-striped">
  <thead>
  <tr>
    <th>SLC Role</th>
    <th>Examples</th>
    <th>Aggregate</th>
    <th>Individual Record Data</th>
	<th>General Student Data</th>
	<th>Restricted Student Data</th>
	<th>Actions</th>
  </tr>
  </thead>

  <tbody>
<% @roles.each do |role| %>
  <tr>
    <td><%= role.name %></td>
    <td><%= role.examples %></td>
	<td><%= role.aggregate %></td>
	<td><%= role.individual %></td>
	<td><%= role.general %></td>
	<td><%= role.restricted %></td>
  </tr>
<% end %>
  </tbody>
</table>

<br />
