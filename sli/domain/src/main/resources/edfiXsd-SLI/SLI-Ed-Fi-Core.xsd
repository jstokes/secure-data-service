    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="SLC-StudentCompetencyExtension">
    <xs:annotation>
      <xs:documentation/>
        <xs:appinfo>
          <sli:recordType>studentCompetency</sli:recordType>
        </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="SLC-StudentCompetencyRestriction">
