        </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ReferenceType">
        <xs:sequence>
          <xs:element name="GradebookEntryIdentity" type="SLC-GradebookEntryIdentityType" minOccurs="0">
            <xs:annotation>
              <xs:documentation/>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="SLC-GradebookEntryIdentityType">
    <xs:annotation>
      <xs:documentation/>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="GradebookEntryType" type="xs:string">
        <xs:annotation>
          <xs:documentation/>
            <xs:appinfo>
