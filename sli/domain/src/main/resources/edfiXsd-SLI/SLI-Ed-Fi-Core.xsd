      <xs:documentation/>
      <xs:appinfo>
        <sli:recordType>studentGradebookEntry</sli:recordType>
      </xs:appinfo>
    </xs:annotation>  
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
