{
    "entities": {
        "grade": {
            "POST": [
                {
                    "ADD_FIELD": {
                        "fieldName": "schoolYear",
                        "ruleSet": [
                            {
                                "rank": 1,
                                "rule": {
                                    "collection": "grade",
                                    "field": "studentSectionAssociationId"
                                }
                            },
                            {
                                "rank": 2,
                                "rule": {
                                    "collection": "studentSectionAssociation",
                                    "field": "sectionId"
                                }
                            },
                            {
                                "rank": 3,
                                "rule": {
                                    "collection": "section",
                                    "field": "courseOfferingId"
                                }
                            },
                            {
                                "rank": 4,
                                "rule": {
                                    "collection": "courseOffering",
                                    "field": "sessionId"
                                }
                            },
                            {
                                "rank": 5,
                                "rule": {
                                    "collection": "session",
                                    "field": "schoolYear"
                                }
                            }
                        ]
                    }
                }
            ]
        },
        "reportCard": {
            "POST": [
                {
                    "ADD_FIELD": {
                        "fieldName": "schoolYear",
                        "ruleSet": [
                            {
                                "rank": 1,
                                "rule": {
                                    "collection": "reportCard",
                                    "field": "gradingPeriodId"
                                }
                            },
                            {
                                "rank": 2,
                                "rule": {
                                    "collection": "gradingPeriod",
                                    "field": "gradingPeriodIdentity,schoolYear"
                                }
                            }
                        ]
                    }
                }
            ]
        },
        "studentAcademicRecord": {
            "POST": [
                {
                    "ADD_FIELD": {
                        "fieldName": "schoolYear",
                        "ruleSet": [
                            {
                                "rank": 1,
                                "rule": {
                                    "collection": "studentAcademicRecord",
                                    "field": "sessionId"
                                }
                            },
                            {
                                "rank": 2,
                                "rule": {
                                    "collection": "session",
                                    "field": "schoolYear"
                                }
                            }
                        ]
                    }
                }
            ]
        },
        "attendance": {
            "POST": [
                {
                    "ATTENDANCE": {
                        "operation": "unwrap"
                    }
                }
            ],
            "GET": [
                {
                    "ATTENDANCE": {
                        "operation": "wrap"
                    }
                }
            ]
        }
    }
}