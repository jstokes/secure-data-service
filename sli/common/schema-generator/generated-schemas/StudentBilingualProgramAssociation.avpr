{
  "type" : "record",
  "name" : "StudentBilingualProgramAssociation",
  "namespace" : "org.slc.sli.domain",
  "doc" : "This association is the StudentProgramAssociation specialization particular for Bilingual programs.  The StudentProgramAssociation represents the program(s) that a student participates in or receives services from.",
  "fields" : [ {
    "name" : "studentReference",
    "type" : "org.slc.sli.domain.StudentReferenceType"
  }, {
    "name" : "programReference",
    "type" : "org.slc.sli.domain.ProgramReferenceType"
  }, {
    "name" : "beginDate",
    "type" : {
      "type" : "string",
      "pattern" : "\\d\\d\\d\\d-\\d\\d-\\d\\d",
      "xsd-type" : "date"
    }
  }, {
    "name" : "endDate",
    "type" : [ "null", {
      "type" : "string",
      "pattern" : "\\d\\d\\d\\d-\\d\\d-\\d\\d",
      "xsd-type" : "date"
    } ]
  }, {
    "name" : "reasonExited",
    "type" : [ "null", "org.slc.sli.domain.enum.ReasonExitedType" ]
  }, {
    "name" : "educationOrganizationReference",
    "type" : "org.slc.sli.domain.EducationalOrgReferenceType"
  }, {
    "name" : "bilingualProgram",
    "type" : "org.slc.sli.domain.enum.BilingualProgramType"
  } ]
}