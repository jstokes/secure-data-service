<?xml version="1.0" encoding="UTF-8"?>
<web-app id="SLISample" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
  http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
  
   <servlet>
    <servlet-name>sample</servlet-name>
    <servlet-class>org.slc.sli.api.client.sample.SLISample</servlet-class>
    
    <init-param>
      <param-name>apiServerURL</param-name>
      <param-value>http://localhost:8080</param-value>
    </init-param>

    <init-param>
      <param-name>clientId</param-name>
      <param-value>vavedRa9uB</param-value>
    </init-param>

    <init-param>
      <param-name>clientSecret</param-name>
      <param-value>phEdAchetESedRewatRegeQeradeC5tUStEXeCa6UJ3JefuC</param-value>
    </init-param>

    <init-param>
      <param-name>callbackURL</param-name>
      <param-value>http://localhost:8080/sample/callback</param-value>
    </init-param>

    <load-on-startup>1</load-on-startup>
     
  </servlet>

  <servlet-mapping>
    <servlet-name>sample</servlet-name>
    <url-pattern>/sample/*</url-pattern>
  </servlet-mapping>
  
</web-app>
