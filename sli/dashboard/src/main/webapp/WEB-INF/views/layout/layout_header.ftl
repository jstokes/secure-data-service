<style>



 .div_main {

	background-color: #444545;

	width : auto;

	height: 100px;

}



.logo_main, .header_right {

    float: left;

    width: 50%;

}

.header_right ul {

    color: #FFFFFF;

    float: right;

    font-size: 16px;

    margin: 12px 0 0;

    padding: 0;

}

.header_right ul li {

    float: left;

    list-style-type: none;

    padding-left: 10px;

    padding-right: 10px;

}

.header_right ul li a {

    color: #FFFFFF;

    text-decoration: none;

}

.header_right ul li {

    border-right: 1px solid #8F8F8F;

}

.header_right ul li.username {

    color: #2CC2E2;

}

.header_right ul li.login {

    border-right: medium none;

}



#footer {

    padding-bottom: 5px;

    padding-top: 5px;

    text-align: left;

}

#footer .footer_wrap {

    border-top: 2px solid #E5E5E5;

    margin: 0 auto;

    width: 980px;

}

#footer .footer_wrap p {

    margin: 0;

}

#footer a {

    color: #CCCCCC;

}





ul.menu_n {list-style:none; margin:0; padding:0;color: #FFFFFF;float: right;font-size: 14px; margin-top:3px;}

ul.menu_n * {margin:0; padding:0}

ul.menu_n a {display:block; color:#000; text-decoration:none;padding-right:15px; padding-left:5px;}

ul.menu_n li {position:relative; float:left; padding-right:10px; padding-left:2px;}

ul.menu_n ul {

position:absolute;

top:20px; display:none; 

list-style:none; 

background:url(../images/custom/menu_arrow1.png) top no-repeat;

border:none;

width:162px;

font-size: 14px;

padding-top:10px;



-webkit-box-shadow: 0px 10px 9px rgba(50, 50, 50, 0.26);

-moz-box-shadow:    0px 10px 9px rgba(50, 50, 50, 0.26);

box-shadow:         0px 10px 9px rgba(50, 50, 50, 0.26);



-webkit-border-bottom-right-radius: 5px;

-webkit-border-bottom-left-radius: 5px;

-moz-border-radius-bottomright: 5px;

-moz-border-radius-bottomleft: 5px;

border-bottom-right-radius: 5px;

border-bottom-left-radius: 5px;

z-index:200;

display:none;

right:0;

}





ul.menu_n li:hover ul{

	display:block;

}





ul.menu_n ul li {

	

	position:relative;

	margin:0;

	border:solid 1px #C5C5C5; 

	padding:0; 

	border-top:none;

	float:none;

	background:#FFFFFF;

	border-bottom:none;

}

ul.menu_n ul li a {display:block; padding:5px 10px 5px; color:#55556A;}

ul.menu_n ul li a:hover { background:#CCCCCC; color:#000035;}

ul.menu_n ul ul {left:148px; top:-1px}

ul.menu_n .menulink { color: #FFFFFF;}

ul.menu_n .menulink:hover, ul.menu_n .menuhover {}



ul.menu_n .first_item{

	border:none;

}

/*ul.menu_n .first_item a.menulink{

	background:url(../images/custom/arrow.png) no-repeat right center;

	color:#2CC2E2;

}*/

ul.menu_n .last_item{

	border:none;

	border-left:#FFFFFF solid 1px;

}

ul.menu_n a.menulink{

	background:url(../images/custom/arrow_w.png) no-repeat right center;

} 



ul.menu_n .first_item ul{

	

}



ul.menu_n ul li.last{

	-webkit-border-bottom-right-radius: 5px;

	-webkit-border-bottom-left-radius: 5px;

	-moz-border-radius-bottomright: 5px;

	-moz-border-radius-bottomleft: 5px;

	border-bottom-right-radius: 5px;

	border-bottom-left-radius: 5px;

}

</style>

