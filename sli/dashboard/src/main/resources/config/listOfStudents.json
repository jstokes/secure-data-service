{
  id : "listOfStudents",
  type : "PANEL",
  data :{
    lazy: true,
    entity: "listOfStudents",
    alias: "listOfStudents",
    params: {
      assessmentFilter: {"ISAT Reading": "MOST_RECENT_RESULT", 
                         "ISAT Writing": "MOST_RECENT_RESULT", 
                         "DIBELS Next": "MOST_RECENT_WINDOW",
                         "TRC": "MOST_RECENT_WINDOW",
                         "SAT Reading": "HIGHEST_EVER",
                         "SAT Writing": "HIGHEST_EVER",
                         "AP Language": "HIGHEST_EVER",
                         "AP Literature": "HIGHEST_EVER" }
    }
  },
  root: 'students',
  items : [
    {name: "Middle School ELA View", items: [
      {name: "Student", field: "name.fullName"},
      {name: "ISAT Reading", items:[
        {name: "Perf. Lvl.", field: "assessments.ISAT Reading.perfLevel"},
        {name: "SS", field: "assessments.ISAT Reading.Scale score"},
        {name: "Lexile Score", field: "assessments.ISAT Reading.Other"}]},
      {name: "ISAT Writing (most recent)", items:[   
        {name: "Perf. Lvl.", field: "assessments.ISAT Writing.perfLevel"},
        {name: "SS", field: "assessments.ISAT Writing.Scale score"}]},
      {name: "Attendance (current school year)", items:[   
        {name: "Absence Count", field: "attendances.absenceCount",  formatter: DashboardUtil.Grid.Formatters.CutPointReverse, params:{cutPoints:{0:{color:'#7FC124'}, 5:{color:  '#DFC836'}, 10:{color:'#DFC836'}, 11:{color:'#B40610'}}}},
        {name: "Attendance Rate %", field: "attendances.attendanceRate", formatter: DashboardUtil.Grid.Formatters.CutPoint, params:{cutPoints:{89:{color:'#B40610'}, 94:{color:'#DFC836'}, 98:{color:'#7FC124'}, 100:{color:'#438746'}}}},
        {name: "Tardy Count", field: "attendances.tardyCount", formatter: DashboardUtil.Grid.Formatters.CutPointReverse, params:{cutPoints:{0:{color:'#7FC124'}, 5:{color:  '#DFC836'}, 10:{color:'#DFC836'}, 11:{color:'#B40610'}}}},
        {name: "Tardy Rate %", field: "attendances.tardyRate", formatter: DashboardUtil.Grid.Formatters.CutPointReverse, params:{cutPoints:{0:{color:'#7FC124'}, 5:{color:  '#DFC836'}, 10:{color:'#DFC836'}, 11:{color:'#B40610'}}}}]}
    ]},
    {name: "Early Literacy View", items: [
      {name: "Student", field: "name.fullName"},
      {name: "DIBELS Next", items:[
        {name: "Perf. Lvl.", field: "assessments.DIBELS Next.perfLevel"}]},
      {name: "Reading", items:[          
        {name: "RL", field: "assessments.TRC.readingLevel"},
        {name: "Prof. Level", field: "assessments.TRC.profLevel"}]},
      {name: "Attendance", items:[   
        {name: "Absence Count", field: "attendances.absenceCount",  formatter: DashboardUtil.Grid.Formatters.CutPointReverse, params:{cutPoints:{0:{color:'#7FC124'}, 5:{color:  '#DFC836'}, 10:{color:'#DFC836'}, 11:{color:'#B40610'}}}},
        {name: "Tardy Count", field: "attendances.tardyCount", formatter: DashboardUtil.Grid.Formatters.CutPointReverse, params:{cutPoints:{0:{color:'#7FC124'}, 5:{color:  '#DFC836'}, 10:{color:'#DFC836'}, 11:{color:'#B40610'}}}}]}
    ]},
    {name: "College Ready ELA View", items: [
      {name: "Student", field: "name.fullName"},
      {name: "Reading Test Scores (Highest)", items:[
        {name: "SAT", field: "assessments.SAT Reading.x"},
        {name: "%ile", field: "assessments.SAT Reading.percentile"}]},   
      {name: "Writing Test Scores (Highest)", items:[          
        {name: "SAT", field: "assessments.SAT Writing.x"},
        {name: "%ile", field: "assessments.SAT Writing.percentile"}]},
      {name: "AP Eng. Exam Scores (Highest)", items:[          
        {name: "Lang.", field: "assessments.AP Language.x"},
        {name: "Lit.", field: "assessments.AP Literature.x"}]},                   
      {name: "Attendance", items:[   
        {name: "Absence Count", field: "attendances.absenceCount",  formatter: DashboardUtil.Grid.Formatters.CutPointReverse, params:{cutPoints:{0:{color:'#7FC124'}, 5:{color:  '#DFC836'}, 10:{color:'#DFC836'}, 11:{color:'#B40610'}}}},
        {name: "Attendance Rate %", field: "attendances.attendanceRate", formatter: DashboardUtil.Grid.Formatters.CutPoint, params:{cutPoints:{89:{color:'#B40610'}, 94:{color:'#DFC836'}, 98:{color:'#7FC124'}, 100:{color:'#438746'}}}},
        {name: "Tardy Count", field: "attendances.tardyCount", formatter: DashboardUtil.Grid.Formatters.CutPointReverse, params:{cutPoints:{0:{color:'#7FC124'}, 5:{color:  '#DFC836'}, 10:{color:'#DFC836'}, 11:{color:'#B40610'}}}},
        {name: "Tardy Rate %", field: "attendances.tardyRate", formatter: DashboardUtil.Grid.Formatters.CutPointReverse, params:{cutPoints:{0:{color:'#7FC124'}, 5:{color:  '#DFC836'}, 10:{color:'#DFC836'}, 11:{color:'#B40610'}}}}]}
    ]}
  ] 
}