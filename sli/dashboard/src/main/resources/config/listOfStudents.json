{
  id : "listOfStudents",
  type : "PANEL",
  data :{
    lazy: true,
    entity: "listOfStudents",
    alias: "listOfStudents",
    params: {
      assessmentFilter: {"ISAT Reading": "MOST_RECENT_RESULT", 
                         "ISAT Writing": "MOST_RECENT_RESULT", 
                         "DIBELS Next": "MOST_RECENT_WINDOW",
                         "TRC": "MOST_RECENT_WINDOW",
                         "SAT Reading": "HIGHEST_EVER",
                         "SAT Writing": "HIGHEST_EVER",
                         "AP Language": "HIGHEST_EVER",
                         "AP Literature": "HIGHEST_EVER" }
    }
  },
  root: 'students',
  items : [
    {name: "Middle School ELA View", items: [
      {name: "Student", width: 200, field: "name.fullName", formatter:'showlink', params: {baseLinkUrl:'student', idName:'id', target:"_blank", showAction=""}},
      {name: "ISAT Reading", items:[
        {name: "Perf. Lvl.", field: "assessments.ISAT Writing.perfLevel", width:100},
        {name: "SS", field: "assessments.ISAT Writing.Scale score", width:100}]},
      {name: "Attendance (current school year)", items:[   
        {name: "Absence Count", field: "attendances.absenceCount", width:100, formatter: DashboardUtil.Grid.Formatters.CutPointReverse, params:{cutPoints:{0:{color:'#7FC124'}, 5:{color:  '#DFC836'}, 10:{color:'#DFC836'}, 11:{color:'#B40610'}}}},
        {name: "Attendance Rate %", field: "attendances.attendanceRate", width:100, formatter: DashboardUtil.Grid.Formatters.CutPoint, params:{cutPoints:{90:{color:'#B40610'}, 95:{color:'#7FC124'}, 99:{color:'#438746'}}}},
        {name: "Tardy Count", field: "attendances.tardyCount", width:100},
        {name: "Tardy Rate %", field: "attendances.tardyRate", width:100}]}
    ]},
    {name: "Early Literacy View", items: [
      {name: "Student", field: "name.fullName", width:100},
      {name: "DIBELS Next", items:[
        {name: "Perf. Lvl.", field: "assessments.DIBELS Next.perfLevel", width:100}]},
      {name: "Reading", items:[          
        {name: "RL", field: "assessments.TRC.readingLevel", width:100},
        {name: "Prof. Level", field: "assessments.TRC.profLevel", width:100}]},
      {name: "Attendance", items:[   
        {name: "Absence Count", field: "attendances.absenceCount", width:100, formatter: DashboardUtil.Grid.Formatters.CutPointReverse, params:{cutPoints:{0:{color:'#7FC124'}, 5:{color:  '#DFC836'}, 10:{color:'#DFC836'}, 11:{color:'#B40610'}}}},
        {name: "Tardy Count", field: "attendances.tardyCount", width:100}]}
    ]},
    {name: "College Ready ELA View", items: [
      {name: "Student", field: "name.fullName", width:100},
      {name: "Reading Test Scores (Highest)", items:[
        {name: "SAT", field: "assessments.SAT Reading.x", width:100},
        {name: "%ile", field: "assessments.SAT Reading.percentile", width:100}]},   
      {name: "Writing Test Scores (Highest)", items:[          
        {name: "SAT", field: "assessments.SAT Writing.x", width:100},
        {name: "%ile", field: "assessments.SAT Writing.percentile", width:100}]},
      {name: "AP Eng. Exam Scores (Highest)", items:[          
        {name: "Lang.", field: "assessments.AP Language.x", width:100},
        {name: "Lit.", field: "assessments.AP Literature.x", width:100}]},                   
      {name: "Attendance", items:[   
        {name: "Absence Count", width: 100, field: "attendances.absenceCount",  formatter: DashboardUtil.Grid.Formatters.CutPointReverse, params:{cutPoints:{0:{color:'#7FC124'}, 5:{color:'#DFC836'}, 10:{color:'#DFC836'}, 11:{color:'#B40610'}}}},
        {name: "Attendance Rate %", width: 100, field: "attendances.attendanceRate", formatter: DashboardUtil.Grid.Formatters.CutPoint, params:{cutPoints:{90:{color:'#B40610'}, 95:{color:'#7FC124'}, 99:{color:'#438746'}}}},
        {name: "Tardy Count", width: 100, field: "attendances.tardyCount"},
        {name: "Tardy Rate %", width: 100, field: "attendances.tardyRate"}]}
    ]}
  ] 
}