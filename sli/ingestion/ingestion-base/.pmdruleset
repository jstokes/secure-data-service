   <rule ref="rulesets/basic.xml/MisplacedNullCheck">
      <priority>1</priority>
      <example><![CDATA[public class Foo {
 void bar() {
  if (a.equals(baz) && a != null) {}
 }
}

public class Foo {
 void bar() {
  if (a.equals(baz) || a == null) {}
 }
}

public class Foo {
 void bar() {
  if (a.equals(baz) && a != null) {}
 }
}

public class Foo {
 void bar() {
  if (a.equals(baz) || a == null) {}
 }
}]]></example>
   </rule>
