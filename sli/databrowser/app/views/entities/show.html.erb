<h2>Viewing information for <%= params[:other] %>
<p><%= link_to 'Back', :back %></p>
<% if @entities.size == 1 %>
	<%= render :partial => "single_record_details", :locals => {:entity => @entities.first} %>
<%else%>
	<%= render :partial => "multi_record_table", :locals => {:entities => @entities} %>
<% end %>