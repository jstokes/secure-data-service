  xmlns="http://ed-fi.org/0100" xsi:schemaLocation="http://ed-fi.org/0100 Interchange-EducationOrganization.xsd">

  
  <!-- Illustrative use of DELETE -->
  <Action ActionType="DELETE" Cascade="true">

    <EducationalOrgReference>
      <EducationalOrgIdentity>
	    <StateOrganizationId>STANDARD-SEA</StateOrganizationId>
      </EducationalOrgIdentity>
    </EducationalOrgReference>

    <EducationalOrgReference>
      <EducationalOrgIdentity>
	    <StateOrganizationId>South Daybreak Elementary</StateOrganizationId>
      </EducationalOrgIdentity>
    </EducationalOrgReference>

    <CourseReference>
      <CourseIdentity>
        <EducationalOrgReference>
          <EducationalOrgIdentity>
            <StateOrganizationId>South Daybreak Elementary</StateOrganizationId>
          </EducationalOrgIdentity>
        </EducationalOrgReference>
        <UniqueCourseId>3rd Grade Homeroom</UniqueCourseId>
      </CourseIdentity>
    </CourseReference>

    <LocationReference>
      <LocationIdentity>
        <ClassroomIdentificationCode>Sample Class</ClassroomIdentificationCode>
        <StateOrganizationId>Sample School</StateOrganizationId>
      </LocationIdentity>
    </LocationReference>
    
   </ProgramReference>
     <ProgramIdentity>
     	<programId>ACC-TEST-PROG-2</programId>
     </ProgramIdentity>
   <ProgramReference>
	   	

  </Action>


  <!-- Illustrative use of MOVE -->
  <Action ActionType="MOVE">
    
    <!-- Ed-Fi domain data follows... -->
    
    <!-- For MOVE, all we need the (old) key information to identify -->
    <StudentReference>
      <StudentIdentity>
        <StudentUniqueStateId>1234678</StudentUniqueStateId>
      </StudentIdentity>
    </StudentReference>
    
    <!-- ... followed by the new data, which may change the key -->
    <Student>
      <StudentUniqueStateId>98765432</StudentUniqueStateId>
      <Name>
        <FirstName>Jessie</FirstName>
        <MiddleName>Kory</MiddleName>
        <LastSurname>Mcgowan</LastSurname>
      </Name>
      <Sex>Male</Sex>
      <BirthData>
        <BirthDate>1988-01-12</BirthDate>
      </BirthData>
      <Address AddressType="Home">
        <StreetNumberName>960 Marks Ct</StreetNumberName>
        <City>New York</City>
        <StateAbbreviation>NY</StateAbbreviation>
        <PostalCode>10292</PostalCode>
      </Address>
      <ElectronicMail EmailAddressType="Home/Personal">
        <EmailAddress>2895@fakemail.com</EmailAddress>
      </ElectronicMail>
      <HispanicLatinoEthnicity>false</HispanicLatinoEthnicity>
      <Race>
        <RacialCategory>Asian</RacialCategory>
      </Race>
      <SchoolFoodServicesEligibility>Full price</SchoolFoodServicesEligibility>
      <LimitedEnglishProficiency>NotLimited</LimitedEnglishProficiency>
      <LoginId>2895@fakemail.com</LoginId>
    </Student>
    
  </Action>
  
</InterchangeEducationOrganization>
