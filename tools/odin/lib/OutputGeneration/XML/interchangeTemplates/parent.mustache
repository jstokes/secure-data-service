<?xml version="1.0" encoding="UTF-8"?>
<InterchangeStudentParent xmlns:p="htt//ed-fi.org/0100" xmlns:xsi="htt//www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="htt//ed-fi.org/0100 SLI-Interchange-StudentParent.xsd ">
	<Parent>
		<ParentUniqueStateId>{{studentId}}</ParentUniqueStateId>
		<Name Verification="{{nameVerification}}">
			{{#prefix}}
			<PersonalTitlePrefix>{{prefix}}</PersonalTitlePrefix>
			{{/prefix}}
			<FirstName>{{firstName}}</FirstName>
			{{#middleName}}
			<MiddleName>{{middleName}}</MiddleName>
			{{/middleName}}
			<LastSurname>{{lastName}}</LastSurname>
			{{#suffix}}
			<GenerationCodeSuffix>{{suffix}}</GenerationCodeSuffix>
			{{/suffix}}
		</Name>
		{{#otherName}}
		<OtherName OtherNameType="{{otherNameType}}">
			{{#prefix}}
			<PersonalTitlePrefix>{{prefix}}</PersonalTitlePrefix>
			{{/prefix}}
			<FirstName>{{firstName}}</FirstName>
			{{#middleName}}
			<MiddleName>{{middleName}}</MiddleName>
			{{/middleName}}
			<LastSurname>{{lastName}}</LastSurname>
			{{#suffix}}
			<GenerationCodeSuffix>{{suffix}}</GenerationCodeSuffix>
			{{/suffix}}
		</OtherName>
		{{/otherName}}
		<Sex>{{sex}}</Sex>
		<BirthData>
			<BirthDate>{{birthDay}}</BirthDate>
		</BirthData>
		<Address AddressType="Home">
			<StreetNumberName>{{address}}</StreetNumberName>
			<City>{{city}}</City>
			<StateAbbreviation>{{state}}</StateAbbreviation>
			<PostalCode>{{postalCode}}</PostalCode>
		</Address>
		<ElectronicMail EmailAddressType="Home/Personal">
			<EmailAddress>{{email}}</EmailAddress>
		</ElectronicMail>
		{{#loginId}}
		<LoginId>{{loginId}}</LoginId>
		{{/loginId}}
	</Parent>
</InterchangeStudentParent>
