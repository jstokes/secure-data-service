{{#entity}}
  <GraduationPlan>
    <GraduationPlanType>{{type}}</GraduationPlanType>
    <IndividualPlan>{{individual}}</IndividualPlan>
    <TotalCreditsRequired CreditConversion="0" CreditType="Semester hour credit">
      <Credit>{{total_credits}}</Credit>
    </TotalCreditsRequired>
    {{#subjects}}
    <CreditsBySubject>
      <SubjectArea>{{subject}}</SubjectArea>
      <Credits CreditConversion="0" CreditType="Semester hour credit">
        <Credit>{{credits}}</Credit>
      </Credits>
    </CreditsBySubject>
    {{/subjects}}
    {{#courses}}
    <CreditsByCourse>
      {{#course}}
      <CourseCode IdentificationSystem="CSSC course code" AssigningOrganizationCode="{{ed_org_id}}">
        <ID>{{id}}</ID>
      </CourseCode>
      {{/course}}
      <Credits CreditConversion="0" CreditType="Semester hour credit">
        <Credit>{{credits}}</Credit>
      </Credits>
      <GradeLevel>Twelfth grade</GradeLevel>
    </CreditsByCourse>
    {{/courses}}
    <EducationOrganizationReference>
        <EducationalOrgIdentity>
          <StateOrganizationId>{{ed_org_id}}</StateOrganizationId>
        </EducationalOrgIdentity>
    </EducationOrganizationReference>
  </GraduationPlan>
{{/entity}}
